#include <iostream>
using namespace std;

class BankAccount {
private:
    string accountHolder;
    float balance;
public:
    void createAccount(string name, float initialBalance) {
        accountHolder = name;
        balance = initialBalance;
    }

    void deposit(float amount) {
        balance += amount;
        cout << "Deposited: $" << amount << endl;
    }

